{ 
	"pk" : "{{property.pk}}",
	"name" : "{{property.name}}",
	"price" : "{{property.price}}",
	"type" : "{{property.type}}",
	"min_bedroom_num" : "{{property.min_bedroom_num}}",
	"max_bedroom_num" : "{{property.max_bedroom_num}}",
        "description" : "{{property.description}}",
        "rating" : "{{property.rating}}",
	"latitude" : "{{property.latitude}}",
	"longitude" : "{{property.longitude}}",
	"street" : "{{property.street}}",
	"city" : "{{property.city}}",
	"state" : "{{property.state}}",
	"zip" : "{{property.zip}}",
	"image_url": "{{property.image_url}}",
	"contact_person" : "{{property.contact_person}}",
	"contact_email" : "{{property.contact_email}}",
	"contact_phone" : "{{property.contact_phone}}",
        "image": "{{property.image}}",
        "highest_vote_review": {"author":"{{property.highest_vote_review.author}}",
"votes":"{{property.highest_vote_review.votes}}","content":"{{property.highest_vote_review.content}}"},
	"tags" : [{% for tag in property.tags %}{%spaceless%}{% include 'studentnest/tag.json' %}{%endspaceless%}{% if not forloop.last %}, {% endif %}{% endfor %}],
	"reviews": [{% for review in property.reviews %}{%spaceless%}{% include 'studentnest/review.json' %}{%endspaceless%}{% if not forloop.last %}, {% endif %}{% endfor %}],
	"keywords" : [{% for keyword in property.keywords %}{%spaceless%}{% include 'studentnest/word.json' %}{%endspaceless%}{% if not forloop.last %}, {% endif %}{% endfor %}]
}
